          <argument>
            <kind>1</kind>
            <string>///collision effects

// impact with asteroid
if (!global.warp) {
    // shield flash
    shield_angle = point_direction(x, y, other.x, other.y);
    shield_glow = shield_timer;
    if (object_get_name(other.object_index) == "obj_asteroid") {
        global.shield_power -= 40;
        if (global.shield_power &lt;= 0) global.shield_power = 0;
    } else {
        global.shield_power -= 10;
        if (global.shield_power &lt;= 0) global.shield_power = 0;
    }
    // generate debris if shields are down
    if (global.shield_power &lt;= 0) {
        instance_destroy();
    } else {
        // get player's current speed
        x_speed = lengthdir_x(speed, direction);
        y_speed = lengthdir_y(speed, direction);
        // get asteroid's speed and direction
        asteroid_direction = other.direction;
        asteroid_x_speed = lengthdir_x(100, asteroid_direction);
        asteroid_y_speed = lengthdir_y(100, asteroid_direction);
        // calculate the new x and y speeds for the player
        x_speed += asteroid_x_speed;
        y_speed += asteroid_y_speed;
        // apply the new direction and speed for the player
        direction = point_direction(x, y, x+x_speed, y+y_speed);
        speed = point_distance(x, y, x+x_speed, y+y_speed);
        if (speed &gt; 3) {
            speed = 3;
        }
    }
    // destroy asteroid
    with(other) instance_destroy();
}
</string>
