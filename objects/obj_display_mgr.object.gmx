            <kind>1</kind>
            <string>///Display Properties

ideal_w = 800;
ideal_h = 450;
zoom = 1;
max_zoom = 1;
max_w = display_get_width();
max_h = display_get_height();
aspect = max_w / max_h;
// calculate max zoom
max_zoom = floor(max_w / ideal_w);
if (max_w &lt; max_h) { // portait
    var VIEW_WIDTH = min(ideal_w, max_w);
    var VIEW_HEIGHT = VIEW_WIDTH / aspect;
    //if (VIEW_HEIGHT &amp; 1) VIEW_HEIGHT++;
} else { // landscape
    var VIEW_HEIGHT = min(ideal_h, max_h);
    var VIEW_WIDTH = VIEW_HEIGHT * aspect;
    //if (VIEW_WIDTH &amp; 1) VIEW_WIDTH++;
}

// set for current room
view_wview[0] = floor(VIEW_WIDTH);
view_hview[0] = floor(VIEW_HEIGHT);
view_wport[0] = max_w;
view_hport[0] = max_h;

// set for next room(s)
for (var i = 1; i &lt;= room_last; i++) {
    if (room_exists(i)) {
        view_x = (room_width / 2) - (ideal_w / 2);
        view_y = (room_height / 2) - (ideal_h / 2);
        room_set_view(i, 0, true, view_x, view_y, floor(VIEW_WIDTH), floor(VIEW_HEIGHT), 0, 0, max_w, max_h, 0, 0, 0, 0, -1);
    }
}

surface_resize(application_surface, view_wview[0], view_hview[0]);
window_set_size(ideal_w, ideal_h);
window_center();
room_goto(room_next(room));

window_h = window_get_height();
window_w = window_get_width();

</string>
